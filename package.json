{
  "name": "nativescript-converters",
  "version": "1.0.1",
  "description": "A small utility package with userfull binding converters that can be used in Plain NativeScript applications.",
  "main": "index",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "3.0.0",
      "android": "3.0.0"
    },
    "plugin": {
      "nan": "false",
      "pan": "true",
      "core3": "true",
      "category": "Utilities"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PeterStaev/nativescript-converters"
  },
  "scripts": {
    "tslint": "tslint --project tsconfig.json",
    "tsc": "tsc --project tsconfig.json"
  },
  "keywords": [
    "NativeScript",
    "converters",
    "convert",
    "tangra"
  ],
  "author": "Peter Staev <peter@tangrasoft.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "grunt": "1.0.1",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-exec": "2.0.0",
    "tns-core-modules": "^4.2.0",
    "tslint": "~5.4.3",
    "typescript": "~2.7.2"
  },
  "dependencies": {
    "moment": "^2.22.2"
  }
}
